{
  "name": "root",
  "private": true,
  "version": "2.7.0",
  "scripts": {
    "build:buildScript": "npx babel --config-file ./scripts/babel.config.js scripts/build.ts --out-file scripts/build.js && node scripts/build.js",
    "build:linariaScript": "npx babel --config-file ./scripts/babel.config.js scripts/linaria.ts --out-file scripts/linaria.js && node scripts/linaria.js",
    "build": " yarn build:linariaScript && yarn build:buildScript",
    "postbuild": "rimraf scripts/build.js && rimraf scripts/linaria.js",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@kmart/palette.macro": "1.5.0",
    "@linaria/babel-preset": "^3.0.0-beta.1",
    "@linaria/core": "^3.0.0-beta.1",
    "@linaria/react": "^3.0.0-beta.1",
    "@linaria/shaker": "^3.0.0-beta.1",
    "@types/fs-extra": "^9.0.6",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.166",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.16",
    "@types/normalize-path": "^3.0.0",
    "@types/prettier": "^2.1.6",
    "@types/react": "^17.0.0",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-transform-string-literal-replace": "^1.0.2",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "mkdirp": "^1.0.4",
    "normalize-path": "^3.0.0",
    "prettier": "^2.2.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "react": "^17.0.1"
  }
}
