  "homepage": "https://github.com/kmartinezmedia/kmart",
  "skpm": {
    "main": "sketch/kmart.sketchplugin",
    "manifest": "sketch/manifest.json",
    "assets": [
      "sketch/assets/**/*"
    ]
  },
  "scripts": {
    "setup": "node config/prepare.js",
    "prestart": "npm run setup",
    "start": "webpack --config webpack.config.js",
    "start:all": "concurrently \"babel src --watch -d lib\" \"npm run storybook\" \"npm run sketch:render\"",
    "prebuild": "rimraf lib && npm run setup",
    "build:old": "babel src -d lib",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "sketch:build": "skpm-build",
    "sketch:watch": "skpm-build --watch",
    "sketch:render": "skpm-build --watch --run",
    "sketch:render:once": "skpm-build --run"
  },
